<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:context="http://www.springframework.org/schema/context"
 xmlns:p="http://www.springframework.org/schema/p"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
 



	<bean id="stud" class="com.springCore.Student" scope="prototype">
	<property name="id" value="10"></property>
	<property name="name" value="shivaji"></property>
	<property name="address" value="13/366"></property>
	</bean>

	<bean id="stud1" class="com.springCore.Student" scope="prototype" p:id="11" p:name="sandeep" p:address="13/366"/>
	
	
	
	
	
 </beans>